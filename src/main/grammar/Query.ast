abstract DlLiteralExpr : Literal;

DlEverythingExpr : DlLiteralExpr;
DlNothingExpr    : DlLiteralExpr;
DlTopRoleExpr    : DlLiteralExpr;
DlBottomRoleExpr : DlLiteralExpr;


abstract DlQuantifierExpr : Binary;

DlExistenceExpr : DlQuantifierExpr;
DlUniversalExpr : DlQuantifierExpr;


abstract DlBinaryExpr : Binary;

DlUnionExpr        : DlBinaryExpr;
DlIntersectionExpr : DlBinaryExpr;


DlNegationExpr  : Unary;
DlInversionExpr : Unary;


DlOneExpr : Unary;


DlQueryExpr : Unary;
